{
  "version": 2,
  "$schema": "https://schemas.wp.org/wp/6.2/theme.json",
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "shadow": {
      "defaultPresets": false,
      "presets": [
        {
          "name": "Natural",
          "slug": "natural",
          "shadow": "0px 1px 4px 0px rgba(0, 0, 0, 0.15)"
        },
        {
          "name": "Crisp",
          "slug": "crisp",
          "shadow": "6px 6px 0px rgba(0, 0, 0, 1)"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "color": {
      "link": true,
      "background": true,
      "text": true,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "color": "#ffffff",
          "name": "Foreground",
          "slug": "foreground"
        },
        {
          "color": "#f9fbfc",
          "name": "Background",
          "slug": "background"
        },
        {
          "color": "#ffd500",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#FDC500",
          "name": "Primary",
          "slug": "primary-hover"
        },
        {
          "color": "#00509d",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#003F88",
          "name": "Secondary",
          "slug": "secondary-hover"
        },
        {
          "color": "#00296b",
          "name": "Heading Color",
          "slug": "heading-color"
        },
        {
          "color": "#1f202c",
          "name": "Text Color",
          "slug": "text-color"
        },
        {
          "color": "#EBF2F9",
          "name": "Hero Background",
          "slug": "hero-bg"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fluid": true,
      "lineHeight": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "textTransform": true,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/montserrat_normal_400.ttf"]
            },
            {
              "fontFamily": "Montserrat",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/montserrat_normal_700.ttf"]
            }
          ],
          "fontFamily": "Montserrat",
          "slug": "montserrat"
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "max": "1.125rem",
            "min": "1rem"
          },
          "size": "1.125rem",
          "slug": "small"
        },
        {
          "fluid": {
            "max": "1.625rem",
            "min": "1.25rem"
          },
          "size": "1.625rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "1,625rem",
            "min": "1.475rem"
          },
          "size": "1,625rem",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "2.625rem",
            "min": "1.75rem"
          },
          "size": "2.625rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "max": "3.25rem",
            "min": "1.65rem"
          },
          "size": "3.25rem",
          "slug": "xx-large"
        }
      ]
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"],
      "customSpacingSize": true,
      "spacingScale": {
        "increment": 1.5,
        "mediumStep": 1.5,
        "operator": "*",
        "steps": 7,
        "unit": "rem"
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "custom": {
      "text-line": "1.7",
      "head-line": "1.3"
    },
    "blocks": {}
  },
  "styles": {
    "color": {
      "background": "var:preset|color|background",
      "text": "var:preset|color|text-color"
    },
    "typography": {
      "fontFamily": "var:preset|font-family|montserrat",
      "fontSize": "var:preset|font-size|small",
      "fontWeight": "400",
      "lineHeight": "1.7",
      "fontStyle": "normal"
    },
    "spacing": {
      "blockGap": "30px",
      "padding": {
        "top": "2rem",
        "right": "1.2rem",
        "bottom": "2rem",
        "left": "1.2rem"
      }
    },
    "blocks": {
      "core/site-title": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "core/group": {
        "shadow": "var:preset|shadow|natural"
      }
    },
    "elements": {
      "button": {
        "border": {
          "radius": "8px"
        },
        "shadow": "var:preset|shadow|natural",
        "color": {
          "background": "var:preset|color|primary",
          "text": "var:preset|color|secondary"
        },
        ":hover": {
          "color": {
            "background": "var:preset|color|primary-hover"
          }
        },
        "typography": {
          "fontWeight": "700"
        },
        "spacing": {
          "padding": {
            "top": ".8rem",
            "right": "3rem",
            "bottom": ".8rem",
            "left": "3rem"
          }
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|text-color"
        },
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "color": {
            "text": "var:preset|color|secondary"
          },
          "typography": {
            "textDecoration": "underline"
          }
        }
      },
      "heading": {
        "color": {
          "text": "var:preset|color|heading-color"
        },
        "typography": {
          "fontWeight": "700",
          "lineHeight": "1.3"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xx-large",
          "textTransform": "uppercase"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|small"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textTransform": "uppercase"
        }
      }
    }
  },
  "customTemplates": {},
  "templateParts": {}
}
